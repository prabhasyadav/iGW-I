            <span class="k">if</span> <span class="n">Pe</span><span class="o">==</span><span class="n">np</span><span class="o">.</span><span class="n">inf</span><span class="p">:</span>
                <span class="k">if</span> <span class="n">t_rel</span><span class="o">&lt;</span><span class="mi">1</span><span class="p">:</span>
