    <span class="c1">##rel conc due initial condition </span>
    <span class="k">if</span> <span class="n">Pe</span> <span class="o">==</span> <span class="kc">None</span> <span class="ow">or</span> <span class="n">t_rel</span> <span class="o">==</span> <span class="kc">None</span><span class="p">:</span>
